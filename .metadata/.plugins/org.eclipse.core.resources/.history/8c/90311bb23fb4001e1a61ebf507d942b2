package com.deloitte.entity;

public class Actor {
	
	@Id
	@Column(name = "customer_id")
	@TableGenerator(name = "cus_seq",table="cus_seq_tab", pkColumnName = "cus_pk",
	valueColumnName = "cus-val", pkColumnValue = "cus", 
	initialValue = 1001, allocationSize = 1)
	@GeneratedValue(strategy = GenerationType.TABLE , generator = "cus_seq")
	private int actorId;
	private String name;
	private int height;
	
}
